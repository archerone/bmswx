<import src="../../common/template/offic.wxml"/>
<template is="offic_top"></template>
<view class="container">
  <image style="width:100%;height:150rpx;" src="../../assets/image/offictop.png"></image>
  <view class="type-item" wx:for="{{types}}" wx:key="{{index}}" data-type="{{item.type}}" bindtap="gosubinfo">
  <image class="type-img" src="{{item.img}}" mode="aspectFill"></image>
  <text class="type-title">
    {{item.name}}
  </text>
  </view>
</view>
